<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<HBox fx:id="hBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="playeriSoft.controlador.MaterialRowController">
   <children>
      <CheckBox fx:id="materialCheckBox" alignment="BOTTOM_LEFT" mnemonicParsing="false">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin>
      </CheckBox>
      <Label fx:id="materialLabel" alignment="BOTTOM_LEFT" text="Material" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin>
      </Label>
      <TextField fx:id="materialCantidadTextField" alignment="BOTTOM_RIGHT">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin>
      </TextField>
   </children>
</HBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<?import playeriSoft.vista.CantidadMaterialesTextField?>
<HBox fx:id="hBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="playeriSoft.controlador.MaterialRowController">
   <children>
      <CheckBox fx:id="materialCheckBox" alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#materialCheckBoxClicked" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin>
      </CheckBox>
      <Label fx:id="materialLabel" alignment="BOTTOM_LEFT" text="Material" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin>
      </Label>
      <Pane HBox.hgrow="ALWAYS" />
      <CantidadMaterialesTextField fx:id="materialCantidadTextField" alignment="BOTTOM_RIGHT" editable="false" prefWidth="70.0" promptText="Cantidad">
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin>
      </CantidadMaterialesTextField>
   </children>
</HBox>
